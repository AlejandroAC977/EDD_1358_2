    this.niveles = niveles;
    }

    public boolean estaVacia(){
    
     return; 
    }

    public void enqueue(int p, T dato){

     niveles=new ArregloADT(NumNiveles);
     niveles.setElemento(p, dato);    

    }
  
    public T dequeue(){

     T mayor=null;
     
     
     return mayor;

    }



